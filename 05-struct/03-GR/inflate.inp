#----------------------------------------------------------------
# General simulation parameters

Continue previous simulation: 0
Number of spatial dimensions: 3
Number of time steps: 1
Time step size: 1.0

Save results to VTK format: 1
Name prefix of saved VTK files: inflate
Increment in saving VTK files: 1
Start saving after time step: 1

Increment in saving restart files: 1

Verbose: 1
Warning: 1
Debug: 0

#----------------------------------------------------------------
# Mesh data
Add mesh: msh {
   Mesh file path:    mesh_large/mesh-complete.mesh.vtu
   Fiber direction file path: mesh_large/mesh-complete.mesh.vtu
   Add face: start {
      Face file path: mesh_large/mesh-surfaces/start.vtp
   }
   Add face: end {
      Face file path: mesh_large/mesh-surfaces/end.vtp
   }
   Add face: inside {
      Face file path: mesh_large/mesh-surfaces/inside.vtp
   }
   Add face: outside {
      Face file path: mesh_large/mesh-surfaces/outside.vtp
   }
   Mesh scale factor: 1.0
#   Fiber direction: (1, 0, 0)
}

#----------------------------------------------------------------
# Equations
Add equation: struct {
   Coupled: 1
   Min iterations: 1
   Max iterations: 20
   Tolerance: 1e-7

   Constitutive model: stVK
   Density: 1000.0
   Elasticity modulus: 1000.0
   Poisson ratio: 0.4

   Output: Spatial {
      Displacement: t
      Velocity: t
      Jacobian: t
      Stress: t
      Strain: t
      Cauchy_stress: t
      VonMises_stress: t

   }

   LS type: BICG
   {
      Preconditioner:      FSILS
      Tolerance:           1D-6
      Max iterations:      1000
#      Krylov space dimension: 50
   }

   Add BC: start {
      Type: Dir
      Value: 0.0
      Effective direction: (0, 0, 1)
      Impose on state variable integral: t
      Zero out perimeter: f
   }

   Add BC: end {
      Type: Dir
      Value: 0.0
      Effective direction: (0, 0, 1)
      Impose on state variable integral: t
      Zero out perimeter: f
   }

   Add BC: inside {
      Type: Dir
      Value: 0.0
      Effective direction: (0, 0, 1)
      Impose on state variable integral: t
      Zero out perimeter: f
   }

   Add BC: outside {
      Type: Dir
      Value: 0.0
      Effective direction: (0, 0, 1)
      Impose on state variable integral: t
      Zero out perimeter: f
   }

   # step 1: pre-stressing
   Add BC: inside {
      Type: Neu
      Time dependence: Unsteady
      Temporal values file path: load_pre.dat
      Ramp function: t
      Follower pressure load: t
   }

   # step 2: g&r
   Add BC: inside {
      Type: Neu
      Time dependence: Unsteady
      Temporal values file path: load_gr.dat
      Ramp function: t
      Follower pressure load: t
   }
}
